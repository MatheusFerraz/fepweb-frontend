    "@commitlint/config-conventional": "^7.1.2",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint-plugin-cypress": "^2.10.3",
    "guid-typescript": "^1.0.9",
    "husky": "^1.3.1",
    "codelyzer": "^6.0.2",
