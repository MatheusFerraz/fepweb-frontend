    "@ngx-translate/http-loader": "^7.0.0",
    "@nrwl/angular": "15.6.3",
    "@po-ui/ng-components": "15.15.0",
    "@po-ui/ng-templates": "15.15.0",
    "@totvs/po-theme": "15.15.0",
    "moment": "2.29.4",
    "rxjs": "~7.8.0",
