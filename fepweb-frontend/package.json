{
  "name": "fepweb-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=development",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "test": "jest"
  },
  "dependencies": {
    "@angular/animations": "^15.1.0",
