    "ng-packagr": "15.1.1",
    "nx": "15.6.3",
    "postcss": "^8.3.9",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
    "prettier": "2.8.3",
    "pretty-quick": "^3.1.1",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "~4.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "./**/*.{ts,json,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest-junit": {
    "output": "<rootDir>/junit.xml"
  }
